
{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
	  "host": {
		"projectType": "application",
		"schematics": {
		  "@schematics/angular:component": {
			"style": "scss"
		  }
		},
		"root": "projects/host",
		"sourceRoot": "projects/host/src",
		"prefix": "app",
		"architect": {
		  "build": {
			"builder": "ngx-build-plus:browser",
			"options": {
			  "outputPath": "dist/host",
			  "index": "projects/host/src/index.html",
			  "polyfills": [
				"zone.js"
			  ],
			  "tsConfig": "projects/host/tsconfig.app.json",
			  "inlineStyleLanguage": "scss",
			  "assets": [
				"projects/host/src/favicon.ico",
				"projects/host/src/assets"
			  ],
			  "styles": [
				"projects/host/src/styles.scss"
			  ],
			  "scripts": [],
			  "main": "projects/host/src/main.ts",
			  "extraWebpackConfig": "projects/host/webpack.config.js",
			  "commonChunk": false
			},
			"configurations": {
			  "production": {
				"budgets": [
				  {
					"type": "initial",
					"maximumWarning": "500kb",
					"maximumError": "1mb"
				  },
				  {
					"type": "anyComponentStyle",
					"maximumWarning": "2kb",
					"maximumError": "4kb"
				  }
				],
				"outputHashing": "all",
				"extraWebpackConfig": "projects/host/webpack.prod.config.js"
			  },
			  "development": {
				"optimization": false,
				"extractLicenses": false,
				"sourceMap": true
			  }
			},
			"defaultConfiguration": "production"
		  },
		  "serve": {
			"builder": "ngx-build-plus:dev-server",
			"configurations": {
			  "production": {
				"buildTarget": "host:build:production",
				"extraWebpackConfig": "projects/host/webpack.prod.config.js"
			  },
			  "development": {
				"buildTarget": "host:build:development"
			  }
			},
			"defaultConfiguration": "development",
			"options": {
			  "port": 4200,
			  "publicHost": "http://localhost:4200",
			  "extraWebpackConfig": "projects/host/webpack.config.js"
			}
		  },
		  "extract-i18n": {
			"builder": "ngx-build-plus:extract-i18n",
			"options": {
			  "buildTarget": "host:build",
			  "extraWebpackConfig": "projects/host/webpack.config.js"
			}
		  },
		  "test": {
			"builder": "@angular-devkit/build-angular:karma",
			"options": {
			  "polyfills": [
				"zone.js",
				"zone.js/testing"
			  ],
			  "tsConfig": "projects/host/tsconfig.spec.json",
			  "inlineStyleLanguage": "scss",
			  "assets": [
				"projects/host/src/favicon.ico",
				"projects/host/src/assets"
			  ],
			  "styles": [
				"projects/host/src/styles.scss"
			  ],
			  "scripts": []
			}
		  }
		}
	  },
	  "commons-lib": {
		"projectType": "library",
		"root": "projects/commons-lib",
		"sourceRoot": "projects/commons-lib/src",
		"prefix": "lib",
		"architect": {
		  "build": {
			"builder": "@angular-devkit/build-angular:ng-packagr",
			"options": {
			  "project": "projects/commons-lib/ng-package.json"
			},
			"configurations": {
			  "production": {
				"tsConfig": "projects/commons-lib/tsconfig.lib.prod.json"
			  },
			  "development": {
				"tsConfig": "projects/commons-lib/tsconfig.lib.json"
			  }
			},
			"defaultConfiguration": "production"
		  },
		  "test": {
			"builder": "@angular-devkit/build-angular:karma",
			"options": {
			  "tsConfig": "projects/commons-lib/tsconfig.spec.json",
			  "polyfills": [
				"zone.js",
				"zone.js/testing"
			  ]
			}
		  }
		}
	  }
	},
	"cli": {
	  "analytics": false
	}
  }
  